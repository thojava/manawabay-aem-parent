                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="manawabay/components/text"
                            text="&lt;p style=&quot;text-align: center;&quot;>Targeting a 5 Green star sustainability rating, Mānawa Bay is committed to a number of key sustainability initiatives.&lt;/p>&#xd;&#xa;"
                            textIsRich="true">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <default
