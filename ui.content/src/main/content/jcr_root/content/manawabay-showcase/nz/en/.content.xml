<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <social/>
    <jcr:content
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/manawabay-showcase/settings/wcm/templates/page-content"
        jcr:mixinTypes="[cq:LiveRelationship]"
        jcr:primaryType="cq:PageContent"
        jcr:title="en"
        sling:resourceType="manawabay/components/page"
        pageTitle="MÄnawa Bay Showcase">
        <root
            jcr:mixinTypes="[cq:LiveRelationship]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="manawabay/components/container"
            layout="responsiveGrid">
            <container
                jcr:mixinTypes="[cq:LiveRelationship]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="manawabay/components/container">
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="manawabay/components/title"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="manawabay/components/container"
                    layout="responsiveGrid"/>
            </container>
        </root>
        <cq:LiveSyncConfig
            cq:isDeep="{Boolean}true"
            cq:master="/content/manawabay-showcase/language-masters/en"
            cq:rolloutConfigs="[/libs/msm/wcm/rolloutconfigs/default]"
            jcr:primaryType="cq:LiveCopy"/>
    </jcr:content>
    <components/>
    <content/>
    <pages/>
</jcr:root>
